+// Nagy
+int isOperator(char* name){
+	if (!name)
+		return 0;
+		
+	switch (*name){
+		case '+': case '-' : case '*' : case '/' : case '<' : case '>' : case '=' : case '&' :
+		case '~' : case '^' : case '!' : case '%' : case '[':
+			return 1;
+	};
+	
+	// add extra methods that you want to exclude here
+	if (!strcmp(name, "times"))
+		return 1;
+		
+	return 0;
 }
 
