+import threading
+
+theVar = 1
+
+class MyThread ( threading.Thread ):
+
+	def run ( self ):
+
+		 global theVar
+		 print 'This is thread ' + str ( theVar ) + ' speaking.'
+		 print 'Hello and good bye.'
+		 theVar = theVar + 1
+
+class MyThread2 ( threading.Thread ):
+	def run ( self ):
+		for x in xrange ( 10 ):
+			print x
+		#	integer1 = raw_input( "Enter first integer:\n" )
+
+def myRun( i ):
+	for x in xrange ( i ):
+		MyThread2().start()
+
+myRun(10)
